{
	"manifest_version": 3,
	"name": "YouTube Auto-Focus",
	"version": "1.0.1",
	"description": "-",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*.youtube.com/watch*"
			],
			"js": [
				"content.js"
			],
			"run_at": "document_end"
		}
	],
	"permissions": []
}
